<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.summer.agecompare.mappers">
	<select id="doSelectOne" parameterType="com.summer.agecompare.domain.Agecompare" resultType="com.summer.agecompare.domain.Agecompare">
		SELECT id AS userId,        
		       pwd,      
		       name,  
		       nickname,     
		       gender,     
		       birth, 
		       email,                                 
		       phone,
		       grade,
		       regdt       
		  FROM su_users     
		 WHERE userId = #{userId,jdbcType=VARCHAR}     
	</select>
</mapper>